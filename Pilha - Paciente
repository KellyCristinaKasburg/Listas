public class Paciente {

    String nome, sexo, cpf;
    int idade;

    public Paciente(String nome, String sexo, String cpf, int idade) {
        this.nome = nome;
        this.sexo = sexo;
        this.cpf = cpf;
        this.idade = idade;
    }
    public String getNome() {
        return nome;
    }
    public void setNome(String nome) {
        this.nome = nome;
    }
    public String getSexo() {
        return sexo;
    }
    public void setSexo(String sexo) {
        this.sexo = sexo;
    }
    public String getCpf() {
        return cpf;
    }
    public void setCpf(String cpf) {
        this.cpf = cpf;
    }
    public int getIdade() {
        return idade;
    }
    public void setIdade(int idade) {
        this.idade = idade;
    }
    public String toString() {
        return "Nome: " + this.nome + " Sexo: " + this.sexo
                + "\n Idade: " + this.idade + " Cpf: " + this.cpf;
    }
}

import java.util.Stack;
import java.util.Scanner;

public class Pilha {
    
    public static void main(String[] args) {
        int op;
        Scanner sc = new Scanner(System.in);
        Stack<Paciente> pilha = new Stack();
        String nome, sexo, cpf;
        int idade;

        do {
            System.out.println("Digite uma das opções abaixo: "
                    + "\n 1 - Cadastrar novo paciente "
                    + "\n 2 - Atendimento ao paciente "
                    + "\n 3 - Mostrar fila "
                    + "\n 0 -  Sair ");
            op = sc.nextInt();
            switch (op) {

                case 1:
                    if (pilha.size() < 2) {
                        System.out.print(" Nome: ");
                        nome = sc.nextLine();
                        System.out.println(" CPF: ");
                        cpf = sc.nextLine();
                        System.out.print(" Sexo: ");
                        sexo = sc.nextLine();
                        System.out.print(" Idade: ");
                        idade = sc.nextInt();
                        Paciente pac = new Paciente(nome, sexo, cpf, idade);
                        pilha.add(pac);
                    } else {
                        System.out.println("ERRO            |  Fila Cheia");
                    }
                    break;

                case 2:
                    System.out.println("Aguarde um momento! \nEncaminhando ao atendimento!");
                    System.out.println(pilha.pop());
                    break;

                case 3:
                    System.out.println(" ");
                    System.out.println(pilha.toString());
                    break;

                case 0:
                    System.out.println("Saindo... ");
                    break;

                default:
                    System.out.println(" Opção invalida! "
                            + "\nTente novamente! ");
            }
        } while (op != 0);
    }
}
